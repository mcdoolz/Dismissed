{
  "manifest_version": 3,
  "name": "dismissed",
  "description": "Dismiss LinkedIn job listings by string.",
  "version": "1.0.0",
  "action": {
    "default_icon": "img/icon_small.png",
    "default_popup": "popup.html",
    "default_title": "Dismiss"
  },
  "icons": {
    "16": "img/icon_small.png",
    "48": "img/icon_medium.png",
    "128": "img/icon_large.png"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}